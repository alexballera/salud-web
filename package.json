{
  "name": "boilerplate-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "local": "env-cmd -f .env.local next dev",
    "dev": "env-cmd -f .env.development next dev",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky install .config/husky",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "sso-login": "aws sso login --profile omnisalud",
    "build": "npx next build",
    "build:serverless:dev": "SLS_DEBUG=1 env-cmd -f .env.development sls build --component=nextApp --stage dev",
    "build:serverless:qa": "SLS_DEBUG=1 env-cmd -f .env.qa sls build --component=nextApp --stage dev",
    "deploy:dev": "npm run build:serverless:dev && npm run sso-login && SLS_DEBUG=1 sls --component=nextApp -v --region us-east-1 --stage dev",
    "deploy:qa": "npm run build:serverless:qa && npm run sso-login && SLS_DEBUG=1 sls --component=nextApp -v --region us-east-1 --stage qa"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@date-io/date-fns": "1.3.13",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@material-ui/core": "4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/pickers": "^3.3.10",
    "@mui/icons-material": "^5.4.4",
    "@mui/material": "^5.4.4",
    "@sls-next/serverless-component": "1.19.0",
    "appwrite": "^6.0.1",
    "axios": "^0.21.1",
    "bootstrap": "^5.0.0-beta3",
    "cross-fetch": "^3.1.4",
    "date-fns": "^2.0.0-beta.5",
    "env-cmd": "^10.1.0",
    "formik": "^2.2.8",
    "graphql": "^15.5.1",
    "i18next": "^21.6.0",
    "i18next-browser-languagedetector": "^6.1.2",
    "interweave": "^12.7.2",
    "next": "11.1.2",
    "next-pwa": "^5.2.21",
    "react": "17.0.2",
    "react-code-input": "^3.10.1",
    "react-dom": "17.0.2",
    "react-horizontal-scrolling-menu": "^2.7.0",
    "react-i18next": "^11.15.0",
    "react-swipeable-views": "^0.13.9",
    "react-text-mask": "^5.4.3",
    "sass": "^1.33.0",
    "webpack": "^5.38.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@next/eslint-plugin-next": "^11.1.0",
    "@testing-library/dom": "^7.31.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/jest-axe": "^3.5.1",
    "@types/node": "^15.6.0",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.8",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/react-text-mask": "^5.4.7",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-jest": "^27.0.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-axe": "^5.0.0",
    "msw": "^0.29.0",
    "next-router-mock": "^0.1.3",
    "prettier": "^2.3.0",
    "pretty-quick": "3.1.0",
    "snapshot-diff": "^0.8.1",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "commitlint": {
    "rules": {
      "references-empty": [
        2,
        "never"
      ]
    },
    "parserPreset": {
      "parserOpts": {
        "issuePrefixes": [
          "SLD-"
        ]
      }
    }
  }
}
